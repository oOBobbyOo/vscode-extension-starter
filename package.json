{
  "name": "vscode-extension-starter",
  "displayName": "vscode-extension-starter",
  "type": "module",
  "version": "0.0.2",
  "description": "vscode extension starter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oOBobbyOo/vscode-extension-starter.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.cjs",
  "engines": {
    "vscode": "^1.107.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vscode-extension-starter.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsdown",
    "watch": "tsdown --watch",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@types/node": "^22.19.3",
    "@types/vscode": "^1.107.0",
    "@vitest/ui": "^2.0.0",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "reactive-vscode": "^0.4.1",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  }
}
